[
  {
    "name": "fcdemo-front-app",
    "image": "nginx:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/fcdemo",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "front-app"
      }
    },
    "portMappings": [
      {
        "containerPort": 80,
        "protocol": "tcp"
      }
    ],
    "secrets": [
      {
        "name": "EXAMPLE_KEY",
        "valueFrom": "/app/EXAMPLE_KEY"
      }
    ]
  }
]
